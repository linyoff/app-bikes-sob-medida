<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Prova02"
        tools:targetApi="31">
        <activity
            android:name=".AllClientes"
            android:exported="false"
            android:label="@string/title_activity_all_clientes"
            android:theme="@style/Theme.Prova02" />
        <activity
            android:name=".TodasBicicletas"
            android:exported="false"
            android:label="@string/title_activity_todas_bicicletas"
            android:theme="@style/Theme.Prova02" />
        <activity
            android:name=".AdicionarBicicleta"
            android:exported="false"
            android:label="@string/title_activity_adicionar_bicicleta"
            android:theme="@style/Theme.Prova02" />
        <activity
            android:name=".AdicionarCliente"
            android:exported="false"
            android:label="@string/title_activity_adicionar_cliente"
            android:theme="@style/Theme.Prova02" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Prova02">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>